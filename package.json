{
    "name": "portfolio",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "bunx --bun astro dev",
        "build": "bunx --bun astro build",
        "prebuild": "bun run lint && bun run prettier:check && bun run sitemap.ts",
        "preview": "bunx --bun astro preview",
        "astro": "astro",
        "lint": "bunx --bun eslint . --cache",
        "prettier:fix": "bunx --bun prettier --write \"**/*.{ts,tsx,js,jsx,mdx,astro}\" --cache",
        "prettier:check": "bunx --bun prettier --check \"**/*.{ts,tsx,js,jsx,mdx,astro}\" --cache"
    },
    "dependencies": {
        "@astrojs/react": "^4.4.2",
        "@tailwindcss/vite": "^4.1.18",
        "@types/bun": "^1.3.5",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "astro": "^5.16.6",
        "astro-seo-meta": "^5.2.0",
        "clsx": "^2.1.1",
        "embla-carousel-autoplay": "^8.6.0",
        "embla-carousel-react": "^8.6.0",
        "i18next": "^25.7.3",
        "lucide-react": "^0.562.0",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "react-hook-form": "^7.69.0",
        "react-i18next": "^16.5.0",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@trivago/prettier-plugin-sort-imports": "^6.0.0",
        "@typescript-eslint/parser": "^8.50.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-astro": "^1.5.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "prettier": "^3.7.4",
        "prettier-plugin-astro": "^0.14.1",
        "prettier-plugin-brace-style": "^0.8.2",
        "prettier-plugin-merge": "^0.8.0",
        "typescript-eslint": "^8.50.1"
    }
}
